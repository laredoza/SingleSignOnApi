{
  "DatabaseType": "MsSql",
  "ConnectionStrings": {
    "defaultConnection": "Data Source=.;Initial Catalog=SingleSignOn;User ID=sa;Password=yourStrong(!)Password;"
  },
  "NLog": {
    "internalLogLevel": "Info",
    "internalLogFile": "./log/internal-nlog.txt",
    "extensions": {
      "NLog.Web.AspNetCore": {
        "assembly": "NLog.Web.AspNetCore"
      }
    },
    "targets": {
      "allfile": {
        "type": "File",
        "fileName": "./log/admin.log",
        "layout": "${longdate}|${event-properties:item=EventId_Id}|${uppercase:${level}}|${logger}|${message} ${exception:format=tostring}",
        "archiveEvery": "Day",
        "archiveFileName": "./log/admin-{########}.log",
        "archiveNumbering": "Date",
        "archiveDateFormat": "yyyyMMdd",
        "maxArchiveFiles": "5"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "allfile"
      },
      {
        "logger": "Microsoft.*",
        "maxLevel": "Info",
        "final": "true"
      }
    ]
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Url": {
    "Authority": "http://localhost:5000",
    "CorsUrl": "http://localhost:4200"
  }
}